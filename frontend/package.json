    "@sveltejs/kit": "^2.49.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/svelte": "^5.0.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.0",
    "jsdom": "^24.0.0",
    "msw": "^2.2.0",
    "prettier": "^3.2.0",
    "prettier-plugin-svelte": "^3.1.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.6.0",
    "tslib": "^2.6.0",
    "typescript": "^5.3.0",
    "vite": "^5.4.0",
    "vitest": "^2.0.0"
